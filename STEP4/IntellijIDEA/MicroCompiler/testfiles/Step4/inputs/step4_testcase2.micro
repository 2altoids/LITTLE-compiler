PROGRAM p
BEGIN

	INT a,b,k,g,p,u;
	STRING newline := "\n";
	
	FUNCTION VOID main()
	BEGIN

		a := 1;
		b := 1;
        WHILE (a <= 80)
			IF (b = 1)
				g := 1*a;
				WRITE (g, newline);
			ENDIF
			IF (b = 2)
				p := 2*a;
				WRITE (p, newline);
			ENDIF
			IF (b = 3)
				k := 3*a;
				WRITE (k, newline);
			ELSE
				u := 4*a;
				WRITE (u, newline);
			ENDIF
			b := b + 1;
			a := a + 20;
		ENDWHILE

	END
END
